{
    "version": 2,
    "public": false,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/client/$1"
        }
    ],
    "redirects": [
        {
            "source": "/",
            "destination": "/client/",
            "permanent": true
        }
    ]
}